{
    "version": 2,
    "builds": [
        {
            "src": "/js/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["json/db.json"]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "js/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}